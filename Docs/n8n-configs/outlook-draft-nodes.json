      },
      "credentials": {
        "microsoftOutlookOAuth2Api": {
          "id": "wSLPm1S7vuBelc25",
          "name": "Microsoft Outlook account 3"
        }
      }
    }
  },
  "connections": {
    "Draft Creation Router": {
      "output3": "Outlook Draft - dachevivo",
      "output4": "Outlook Draft - dachevivo2",
      "output5": "Outlook Draft - dachevivo3"
    },
    "All Outlook Draft nodes": {
      "output": "Aggregate Email Metrics"
    }
  }
